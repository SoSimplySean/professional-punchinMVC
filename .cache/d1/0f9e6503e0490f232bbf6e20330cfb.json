sultsView.js","original":{"line":68,"column":10}},{"generated":{"line":69,"column":0},"source":"js/views/resultsView.js","original":{"line":69,"column":9}},{"generated":{"line":70,"column":0},"source":"js/views/resultsView.js","original":{"line":70,"column":7}},{"generated":{"line":70,"column":9},"source":"js/views/resultsView.js","original":{"line":59,"column":6}},{"generated":{"line":71,"column":0},"source":"js/views/resultsView.js","original":{"line":71,"column":5}},{"generated":{"line":71,"column":7},"source":"js/views/resultsView.js","original":{"line":45,"column":4}},{"generated":{"line":72,"column":0},"source":"js/views/resultsView.js","original":{"line":72,"column":3}},{"generated":{"line":78,"column":15},"source":"js/views/resultsView.js","original":{"line":75,"column":15}},{"name":"resultsView","generated":{"line":78,"column":19},"source":"js/views/resultsView.js","original":{"line":75,"column":19}},{"generated":{"line":78,"column":30},"source":"js/views/resultsView.js","original":{"line":75,"column":15}}],"sources":{"js/views/resultsView.js":"const projectList = document.querySelector(`.projects`);\nconst header__user = document.querySelector(`.header__user`);\n\nclass resultsView {\n  addHandlerFavourited(handler, reset) {\n    header__user.addEventListener(\"click\", function () {\n      this.classList.toggle(\"activeUser\");\n      if (this.classList.contains(\"activeUser\")) {\n        handler();\n      } else {\n        reset();\n      }\n    });\n  }\n\n  render(data) {\n    projectList.innerHTML = \"\";\n\n    // For each row (array), create a HTML card\n    data.forEach((project) => {\n      const htmlString = `\n            <li class=\"project-card-container\">\n                <img\n                src=\"${project[3]}\"\n                alt=\"\"\n                class=\"project-card__img\"\n                />\n                <div class=\"project-card__text-container\">\n                <div>\n                <p class=\"project-card__id\" style=\"display: none;\">${project[0]}</p>\n                <h2 class=\"project-card__title\">${project[1]}</h2>\n                <p class=\"project-card__eta\">Est. ${project[2]}</p>\n                </div>\n                <i class=\"project-card__heart bi bi-suit-heart\"></i>\n                </div>\n            </li>`;\n\n      projectList.insertAdjacentHTML(`beforeend`, htmlString);\n    });\n\n    const likeProject = document.querySelectorAll(\".project-card__heart\");\n\n    // FAVOURITE PROJECTS\n    // Putting it here for now since it only works after items load\n    likeProject.forEach(function (item) {\n      // Activate heart if it is in local storage\n      // Gets the ID of the project card\n      const id =\n        item.closest(\"div div\").firstElementChild.firstElementChild.innerText;\n\n      for (let i = 0; i < localStorage.length; i++) {\n        if (id == localStorage.key(i)) {\n          item.classList.remove(\"bi-suit-heart\");\n          item.classList.add(\"bi-suit-heart-fill\");\n          item.classList.add(\"heart\");\n        }\n      }\n\n      item.addEventListener(\"click\", function () {\n        item.classList.toggle(\"bi-suit-heart-fill\");\n        item.classList.toggle(\"bi-suit-heart\");\n        item.classList.toggle(\"heart\");\n\n        // Checks whether the project was saved and adds/removes from local storage\n        if (item.classList.contains(\"heart\")) {\n          localStorage.setItem(id, id);\n        } else {\n          localStorage.removeItem(id, id);\n        }\n      });\n    });\n  }\n}\n\nexport default new resultsView();\n"},"lineCount":null}},"error":null,"hash":"43aa0a4fcaf6e2639f90d501f793b4c4","cacheData":{"env":{}}}