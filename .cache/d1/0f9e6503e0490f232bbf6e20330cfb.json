ultsView.js","original":{"line":57,"column":10}},{"name":"id","generated":{"line":55,"column":36},"source":"js/views/resultsView.js","original":{"line":57,"column":34}},{"generated":{"line":55,"column":38},"source":"js/views/resultsView.js","original":{"line":57,"column":10}},{"name":"id","generated":{"line":55,"column":40},"source":"js/views/resultsView.js","original":{"line":57,"column":38}},{"generated":{"line":55,"column":42},"source":"js/views/resultsView.js","original":{"line":57,"column":10}},{"generated":{"line":56,"column":0},"source":"js/views/resultsView.js","original":{"line":58,"column":9}},{"generated":{"line":57,"column":0},"source":"js/views/resultsView.js","original":{"line":59,"column":7}},{"generated":{"line":57,"column":9},"source":"js/views/resultsView.js","original":{"line":48,"column":6}},{"generated":{"line":58,"column":0},"source":"js/views/resultsView.js","original":{"line":60,"column":5}},{"generated":{"line":58,"column":7},"source":"js/views/resultsView.js","original":{"line":34,"column":4}},{"generated":{"line":59,"column":0},"source":"js/views/resultsView.js","original":{"line":61,"column":3}},{"generated":{"line":65,"column":15},"source":"js/views/resultsView.js","original":{"line":64,"column":15}},{"name":"resultsView","generated":{"line":65,"column":19},"source":"js/views/resultsView.js","original":{"line":64,"column":19}},{"generated":{"line":65,"column":30},"source":"js/views/resultsView.js","original":{"line":64,"column":15}}],"sources":{"js/views/resultsView.js":"const projectList = document.querySelector(`.projects`);\n\nclass resultsView {\n  render(data) {\n    projectList.innerHTML = \"\";\n\n    // For each row (array), create a HTML card\n    data.forEach((project) => {\n      const htmlString = `\n            <li class=\"project-card-container\">\n                <img\n                src=\"${project[3]}\"\n                alt=\"\"\n                class=\"project-card__img\"\n                />\n                <div class=\"project-card__text-container\">\n                <div>\n                <p class=\"project-card__id\" style=\"display: none;\">${project[0]}</p>\n                <h2 class=\"project-card__title\">${project[1]}</h2>\n                <p class=\"project-card__eta\">Est. ${project[2]}</p>\n                </div>\n                <i class=\"project-card__heart bi bi-suit-heart\"></i>\n                </div>\n            </li>`;\n\n      projectList.insertAdjacentHTML(`beforeend`, htmlString);\n    });\n\n    const likeProject = document.querySelectorAll(\".project-card__heart\");\n    console.log(likeProject);\n\n    // FAVOURITE PROJECTS\n    // Putting it here for now since it only works after items load\n    likeProject.forEach(function (item) {\n      // Activate heart if it is in local storage\n      // Gets the ID of the project card\n      const id =\n        item.closest(\"div div\").firstElementChild.firstElementChild.innerText;\n\n      for (let i = 0; i < localStorage.length; i++) {\n        if (id == localStorage.key(i)) {\n          item.classList.remove(\"bi-suit-heart\");\n          item.classList.add(\"bi-suit-heart-fill\");\n          item.classList.add(\"heart\");\n        }\n      }\n\n      item.addEventListener(\"click\", function () {\n        item.classList.toggle(\"bi-suit-heart-fill\");\n        item.classList.toggle(\"bi-suit-heart\");\n        item.classList.toggle(\"heart\");\n\n        // Checks whether the project was saved and adds/removes from local storage\n        if (item.classList.contains(\"heart\")) {\n          localStorage.setItem(id, id);\n        } else {\n          localStorage.removeItem(id, id);\n        }\n      });\n    });\n  }\n}\n\nexport default new resultsView();\n"},"lineCount":null}},"error":null,"hash":"38fdd55dea081d488a69f5a5f5e8cf0b","cacheData":{"env":{}}}